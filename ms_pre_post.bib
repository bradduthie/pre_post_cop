journal = {Genetics},
keywords = {Alleles,Amino Acid Substitution,Amino Acids,Amino Acids: genetics,Biometry,Gene Frequency,Genetic,Genetics,Heritable,Humans,Models,Mutation,Polymorphism,Population,Quantitative Trait,Selection,Single Nucleotide,Time Factors},
month = {jun},
