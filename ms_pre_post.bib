journal = {Heredity},
keywords = {Crosses,Genetic,Inbreeding,Pollen,Pollen: genetics,Pollen: physiology,Pollination,Selection,Silene,Silene: genetics,Silene: physiology},
month = {mar},
number = {2},
pages = {101--112},
pmid = {18698334},
title = {{Evidence for inbreeding depression and post-pollination selection against inbreeding in the dioecious plant Silene latifolia}},
