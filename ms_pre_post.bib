@article{Luong2007,
abstract = {Inbreeding, which increases homozygosity throughout the genome by increasing the proportion of alleles that are identical by descent, is expected to compromise resistance against parasitism. Here, we demonstrate that host inbreeding increases susceptibility to ectoparasitism in a natural fruit fly (Drosophila nigrospiracula) - mite (Macrocheles subbadius) association, and that this effect depends on host genetic background. Moreover, flies generated from reciprocal crosses between susceptible inbred lines exhibited elevated levels of resistance similar to that in the mass-bred base population, confirming in reverse direction the causative link between expected heterozygosity and resistance. We also show that inbreeding reduces the host's ability to sustain energetically expensive behaviours, and that host exhaustion dramatically increases susceptibility. These findings suggest that inbreeding depression for resistance results from an inability to sustain defensive behaviours because of compromised physiological competence.},
archivePrefix = {arXiv},
arxivId = {Thesis/Infectious Disease and Immunity},
