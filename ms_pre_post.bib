pmid = {16612884},
title = {{Sexual conflict over mating and fertilization: an overview}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1569603{\&}tool=pmcentrez{\&}rendertype=abstract},
