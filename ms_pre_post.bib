pages = {137--49},
pmid = {24372600},
title = {{Epistasis, pleiotropy, and the mutation load in sexual and asexual populations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24372600},
volume = {68},
year = {2014}
}
