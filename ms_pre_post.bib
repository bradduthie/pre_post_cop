keywords = {Adaptation,Biological,Biological: genetics,Computer Simulation,Dominant,Environment,Evolution,Extinction,Genes,Genetic,Genetic Loci,Genetic Variation,Hybridization,Inbreeding,Models,Molecular,Mutation},
