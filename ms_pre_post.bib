journal = {Biology Letters},
keywords = {Animals,Biological,Biological Evolution,Gryllidae,Gryllidae: physiology,Inbreeding,Male,Models,Spermatogonia,Spermatogonia: cytology,Western Australia},
month = {apr},
number = {2},
pages = {183--185},
pmid = {18211862},
title = {{No postcopulatory response to inbreeding by male crickets}},
