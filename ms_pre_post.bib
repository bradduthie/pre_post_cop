journal = {Trends in Ecology {\&} Evolution},
keywords = {Animals,Gene Flow,Genetic,Genetic Speciation,Population Dynamics,Selection},
month = {mar},
number = {3},
pages = {145--156},
pmid = {19185951},
publisher = {Elsevier Ltd},
title = {{Ecological explanations for (incomplete) speciation}},
