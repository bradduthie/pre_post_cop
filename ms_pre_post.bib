isbn = {1476-4687 (Electronic)$\backslash$r0028-0836 (Linking)},
issn = {1476-4687},
journal = {Nature},
keywords = {Arctic Regions,Biodiversity,Carbon,Carbon Cycle,Carbon: metabolism,Ecosystem,Nitrogen,Nitrogen Cycle,Nitrogen: metabolism,Plants,Plants: metabolism,Tropical Climate},
number = {7499},
pages = {218--221},
