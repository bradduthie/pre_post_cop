keywords = {Animal,Animal: physiology,Animals,Biological Evolution,Decision Making,Decision Making: physiology,Female,Genetic,Male,Reproduction,Reproduction: genetics,Reproduction: physiology,Selection,Sex Characteristics,Sexual Behavior},
month = {may},
pages = {S19--S25},
pmid = {15795858},
title = {{Evolutionary conflicts of interest: are female sexual decisions different?}},
