@article{Shuker2007,
abstract = {Understanding the evolution of female multiple mating (polyandry) is crucial for understanding sexual selection and sexual conflict. Despite this interest, little is known about its genetic basis or whether genetics influences the evolutionary origin or maintenance of polyandry. Here, we explore the quantitative genetic basis of polyandry in the parasitoid wasp Nasonia vitripennis, a species in which female re-mating has been observed to evolve in the laboratory. We performed a quantitative genetic experiment on a recently collected population of wasps. We found low heritabilities of female polyandry (re-mating frequency after 18 h), low heritability of courtship duration and a slightly higher heritability of copulation duration. However, the coefficients of additive genetic variance for these traits were all reasonably large (CV(A){\textgreater}7.0). We also found considerable dam effects for all traits after controlling for common environment, suggesting either dominance or maternal effects. Our work adds to the evidence that nonadditive genetic effects may influence the evolution of mating behaviour in Nasonia vitripennis, and the evolution of polyandry more generally.},
author = {Shuker, D M and Phillimore, A J and Burton-Chellew, M N and Hodge, S E and West, S A},
doi = {10.1038/sj.hdy.6800897},
file = {:home/abduthie/Dropbox/DuthiePapers/ShukerEtAl2007.pdf:pdf},
isbn = {ISSN 0018-067X},
issn = {0018-067X},
journal = {Heredity},
keywords = {additive genetic variance,heritability,hymenoptera,polyandry,sexual conflict,sexual selection},
number = {2},
pages = {69--73},
pmid = {16985510},
title = {{The quantitative genetic basis of polyandry in the parasitoid wasp, Nasonia vitripennis}},
volume = {98},
year = {2007}
}
