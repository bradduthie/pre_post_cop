@article{Michalczyk2011,
abstract = {The widespread phenomenon of polyandry (mating by females with multiple males) is an evolutionary puzzle, because females can sustain costs from promiscuity, whereas full fertility can be provided by a single male. Using the red flour beetle, Tribolium castaneum, we identify major fitness benefits of polyandry to females under inbreeding, when the risks of fertilization by incompatible male haplotypes are especially high. Fifteen generations after inbred populations had passed through genetic bottlenecks, we recorded increased levels of female promiscuity compared with noninbred controls, most likely due to selection from prospective fitness gains through polyandry. These data illustrate how this common mating pattern can evolve if population genetic bottlenecks increase the risks of fitness depression due to fertilization by sperm carrying genetically incompatible haplotypes.},
annote = {This study includes eight generations of sibling-to-sibling matings (inbred) versus random mating in a large (Ne {\textgreater} 1000) population in red flour beetles. These are two extreme ends of the inbreeding versus outbreeding spectrum, and it's difficult to imagine what kind of species would have this range of choice in inbreeding. Because the benefit of polyandry was only observed in the inbred lines, and the authors suggest that polyandry becomes beneficial when populations become inbred, but is this realistic given the massive amount of inbreeding going on? Our results of a small, more realistically inbred population don't suggest the evolution of polyandry.

Further, fitness is measured here only in terms of reproductive success, and the inclusive fitness benefit of inbreeding is not taken into account, so the fitness of inbreeding females is not necessarily as low as implied by this study.

Overall, the authors find that polandry (defined as mating with five males) is beneficial only in highly inbred populations. In such highly inbred populations, polyandry increases female reproductive success.},
author = {Michalczyk, {\L}ukasz and Millard, Anna L and Martin, Oliver Y and Lumley, Alyson J and Emerson, Brent C and Chapman, Tracey and Gage, Matthew J G},
doi = {10.1126/science.1207314},
file = {:home/abduthie/Dropbox/DuthiePapers/MichalczykEtAl2011.pdf:pdf},
issn = {1095-9203},
journal = {Science},
keywords = {Animal,Animals,Biological Evolution,Female,Fertility,Fertilization,Genetic Fitness,Inbreeding,Male,Reproduction,Sexual Behavior,Tribolium,Tribolium: genetics,Tribolium: physiology},
month = {sep},
number = {6050},
pages = {1739--1742},
pmid = {21940892},
title = {{Inbreeding promotes female promiscuity}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21940892},
volume = {333},
year = {2011}
}
