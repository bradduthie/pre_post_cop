journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {breeding value,extra-pair reproduction,indirect selection,polyandry,quantitative genetics},
number = {1734},
pages = {1700--1708},
pmid = {22113036},
title = {{Indirect selection on female extra-pair reproduction? Comparing the additive genetic value of maternal half-sib extra-pair and within-pair offspring}},
