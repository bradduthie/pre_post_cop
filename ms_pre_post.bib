journal = {Proceedings of the Royal Society B},
keywords = {Animals,Atlantic Ocean,Cohort Studies,DNA Primers,Fisheries,Flounder,Flounder: genetics,Flounder: physiology,Genetic,Genetic Variation,Genetics,Genotype,Heterozygote,Inbreeding,Likelihood Functions,Microsatellite Repeats,Microsatellite Repeats: genetics,Population,Population Density,Population Dynamics,Selection},
