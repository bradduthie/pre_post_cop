doi = {10.1016/j.vaccine.2011.05.040},
file = {:home/abduthie/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Beyer et al. - 2011 - Immunogenicity and safety of inactivated influenza vaccines in primed populations a systematic literature review a.pdf:pdf},
