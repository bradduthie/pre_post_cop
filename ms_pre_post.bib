journal = {Philosophical Transactions of the Royal Society B},
keywords = {behaviour,evolution,theoretical biology},
number = {1687},
pages = {20150085},
title = {{There is no fitness but fitness, and the lineage is its bearer}},
volume = {371},
year = {2016}
}
