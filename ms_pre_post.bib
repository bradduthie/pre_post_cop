@article{Fromhage2010,
abstract = {The evolutionary forces shaping within- and across-species variation in the investment in male and female sex function are still incompletely understood. Despite earlier suggestions that in plants the evolution or cosexuality vs. dioecy, as well as sex allocation among cosexuals, is affected by seed and pollen dispersal, no formal model has explicitly used dispersal distances to address this problem. Here, we present a game-theory model as well as a simulation study that fills in this gap. Our model predicts that dioecy should evolve if seeds and pollen disperse widely and that sex allocation among cosexuals should be biased towards whichever sex function produces more widely dispersing units. Dispersal limitations stabilize cosexuality by reinforcing competition between spatially clumped dispersal units from the same source, leading to saturating fitness returns that render sexual specialization unprofitable. However, limited pollen dispersal can also increase the risk of selfing, thus potentially selecting for dioecy as an outbreeding mechanism. Finally, we refute a recent claim that cosexuals should always invest equally in both sex functions.},
author = {Fromhage, Lutz and Kokko, Hanna},
doi = {10.1111/j.1420-9101.2010.02057.x},
file = {:home/abduthie/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Fromhage, Kokko - 2010 - Spatial seed and pollen games dispersal, sex allocation, and the evolution of dioecy(2).pdf:pdf},
issn = {1420-9101},
journal = {Journal of Evolutionary Biology},
keywords = {Biological,Biological Evolution,Models,Pollen,Pollen: physiology,Seed Dispersal,Seed Dispersal: physiology,Seeds,Seeds: physiology,Sex},
month = {sep},
number = {9},
pages = {1947--1956},
pmid = {20695970},
title = {{Spatial seed and pollen games: dispersal, sex allocation, and the evolution of dioecy}},
volume = {23},
year = {2010}
}
@article{Fromhage2009,
abstract = {The extent to which indirect genetic benefits can drive the evolution of directional mating preferences for more ornamented mates, and the mechanisms that maintain such preferences without depleting genetic variance, remain key questions in evolutionary ecology. We used an individual-based genetic model to examine whether a directional preference for mates with higher genome-wide heterozygosity (H), and consequently greater ornamentation, could evolve and be maintained in the absence of direct fitness benefits of mate choice. We specifically considered finite populations of varying size and spatial genetic structure, in which parent-offspring resemblance in heterozygosity could provide an indirect benefit of mate choice. A directional preference for heterozygous mates evolved under broad conditions, even given a substantial direct cost of mate choice, low mutation rate, and stochastic variation in the link between individual heterozygosity and ornamentation. Furthermore, genetic variance was retained under directional sexual selection. Preference evolution was strongest in smaller populations, but weaker in populations with greater internal genetic structure in which restricted dispersal increased local inbreeding among offspring of neighboring females that all preferentially mated with the same male. These results suggest that directional preferences for heterozygous or outbred mates could evolve and be maintained in finite populations in the absence of direct fitness benefits, suggesting a novel resolution to the lek paradox.},
author = {Fromhage, Lutz and Kokko, Hanna and Reid, Jane M},
doi = {10.1111/j.1558-5646.2008.00575.x},
file = {:home/abduthie/Dropbox/DuthiePapers/FromhageEtAl2009.pdf:pdf},
