journal = {Journal of Theoretical Biology},
month = {oct},
number = {4},
pages = {345--364},
pmid = {10525395},
title = {{Kin competition, the cost of inbreeding and the evolution of dispersal}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10525395},
volume = {200},
year = {1999}
}
