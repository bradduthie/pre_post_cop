issn = {1420-9101},
journal = {Journal of Evolutionary Biology},
keywords = {Biological Evolution,Computer Simulation,Genetic,Genetic Variation,Inbreeding,Models,Plant Physiological Phenomena,Plant Physiological Phenomena: genetics},
