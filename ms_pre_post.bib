journal = {Proceedings of The Royal Society B},
keywords = {ecology,evolution,theoretical biology},
number = {1813},
pages = {20151075},
pmid = {26246549},
title = {{Cooperation, clumping and the evolution of multicellularity}},
url = {http://rspb.royalsocietypublishing.org/lookup/doi/10.1098/rspb.2015.1075},
volume = {282},
year = {2015}
}
