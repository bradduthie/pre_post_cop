journal = {Proceedings of The Royal Society B},
keywords = {Animal,Animals,Chickens,Chickens: genetics,Chickens: physiology,Choice Behavior,Female,Genetic,Inbreeding,Major Histocompatibility Complex,Male,Mating Preference,Polymorphism,Reproduction,Spermatozoa,Spermatozoa: physiology},
number = {1769},
pages = {20131296},
pmid = {24004935},
title = {{Cryptic female choice favours sperm from major histocompatibility complex-dissimilar males}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3768299{\&}tool=pmcentrez{\&}rendertype=abstract},
