issn = {1098-2345},
journal = {American Journal of Primatology},
keywords = {an infant,challenging and,genetic paternity,gibbons,however,hylobates lar,in contrast to a,living,monogamy,mother,pair,polyandry,pool of males is,reliably identifying,s sire from a},
