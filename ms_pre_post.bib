issn = {1878-0326},
journal = {Forensic Science International. Genetics},
month = {may},
pages = {49--54},
pmid = {24572837},
publisher = {Elsevier Ireland Ltd},
title = {{Mixtures with relatives: a pedigree perspective}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24572837},
volume = {10},
year = {2014}
}
