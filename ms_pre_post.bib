journal = {Genetics},
keywords = {Breeding,Chromosome Mapping,Crosses,Epistasis,Evolution,Genes,Genetic,Genetic Markers,Genotype,Heritable,Hybrid Vigor,Lod Score,Models,Molecular,Oryza sativa,Oryza sativa: genetics,Plant,Polymorphism,Quantitative Trait,Restriction Fragment Length,Statistical},
month = {aug},
