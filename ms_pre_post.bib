journal = {American Naturalist},
keywords = {Animals,Biological Evolution,Ecosystem,Furans,Genetic,Genetic Fitness,Genetic Variation,Inbreeding,Lactones,Models,Mutation,Selection},
month = {aug},
number = {2},
pages = {111--122},
pmid = {20545488},
title = {{Ecological determinants of mutation load and inbreeding depression in subdivided populations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20545488},
volume = {176},
year = {2010}
}
