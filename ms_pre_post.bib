journal = {Proceedings of the Royal Society B: Biological Sciences},
keywords = {Sexual Selection Alternative Mating Strategies Mal},
number = {1382},
pages = {747--754},
title = {{Alternative mating tactics and extreme male dimorphism in fig wasps}},
volume = {264},
year = {1997}
}
