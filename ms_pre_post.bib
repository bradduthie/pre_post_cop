issn = {1365-294X},
journal = {Molecular Ecology},
keywords = {Animal,Animals,Databases,Foxes,Foxes: genetics,Genetic,Genetic Variation,Inbreeding,Microsatellite Repeats,Microsatellite Repeats: genetics,Population Density,Reproduction,Reproduction: genetics,Sexual Behavior,Wolves,Wolves: genetics},
month = {dec},
