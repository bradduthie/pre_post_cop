isbn = {0006-3207},
issn = {00063207},
journal = {Biological Conservation},
keywords = {Canis lupus,Conservation policy,Inbreeding depression,Isle Royale},
number = {5},
pages = {1025--1031},
pmid = {38},
title = {{Congenital bone deformities and the inbred wolves (Canis lupus) of Isle Royale}},
volume = {142},
