issn = {1471-0056},
journal = {Nature reviews. Genetics},
keywords = {Amino Acid Substitution,Animals,DNA,DNA: genetics,Evolution,Genetic,Humans,Models,Molecular,Mutagenesis,Mutation,Polymorphism,Single Nucleotide},
month = {aug},
