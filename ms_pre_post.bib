file = {:home/abduthie/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Morris, Bronstein, Wilson - 2003 - Three-way coexistence in obligate mutualist-exploiter interactions the potential role of competiti(2).pdf:pdf},
issn = {0003-0147},
journal = {American Naturalist},
keywords = {Animals,Biological,Biological Evolution,Host-Parasite Interactions,Insects,Larva,Models,Pollen,Population Dynamics,Seeds,Yucca},
month = {jun},
number = {6},
pages = {860--875},
pmid = {12858272},
title = {{Three-way coexistence in obligate mutualist-exploiter interactions: the potential role of competition}},
