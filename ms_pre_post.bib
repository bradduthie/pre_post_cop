journal = {Evolution},
keywords = {Genetic correlation,Inducible defense,Phenotypic plasticity,Reaction norm,Shell thickness,Stress,Waiting time},
number = {12},
pages = {3476--3494},
pmid = {20624174},
title = {{The effects of predation risk on mating system expression in a freshwater snail}},
volume = {64},
year = {2010}
}
