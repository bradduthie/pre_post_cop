journal = {Behavioral Ecology},
keywords = {Genetic similarity,Inbreeding avoidance,Kin recognition,Pedigrees,Random mating},
month = {mar},
number = {3},
pages = {575--584},
title = {{Why some species of birds do not avoid inbreeding: Insights from New Zealand robins and saddlebacks}},
url = {http://www.beheco.oxfordjournals.org/cgi/doi/10.1093/beheco/arp034},
volume = {20},
