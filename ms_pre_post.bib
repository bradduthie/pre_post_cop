journal = {American Naturalist},
keywords = {Animal,Animal: physiology,Animals,Female,Genetic Variation,Inbreeding,Male,Pair Bond,Reproduction,Reproduction: genetics,Reproduction: physiology,Sexual Behavior,Sparrows,Sparrows: genetics,Sparrows: physiology},
month = {feb},
number = {2},
pages = {177--187},
