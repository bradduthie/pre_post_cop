file = {:home/abduthie/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bonte et al. - 2012 - Costs of dispersal.pdf:pdf},
issn = {1469-185X},
journal = {Biological reviews of the Cambridge Philosophical Society},
keywords = {Animals,Biological,Biological Evolution,Demography,Ecosystem,Models,Plants},
